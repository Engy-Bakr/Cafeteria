<%include  ../layout/header.ejs %>
<h1>Users List</h1>
<% if(!msg){ %>
    <h3 class="alert alert-success"><%= msg %></h3>
    <% }%>
<table class="table table-bordered">
    <tr>
        <th>Post ID</th>
    
        <th>
            Title
        </th>
        <th>Action</th>
    </tr>
    <% data.forEach(function(post) { %>
        <tr>
            <td>
                <%= post.id %>
            </td>
            <td>
                <%= post.name %>

            </td>
            <td><a class="btn btn-danger" href="/users/delete/<%= post._id %>">Delete</a>
            <a class="btn btn-primary" href="/users/edit/<%= post._id %>">Edit</a></td>            
        </tr>
        <% }) %>
</table>
<%include  ../layout/footer.ejs %>
